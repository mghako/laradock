# Use the official Nginx image
FROM nginx:latest

#RUN rm /etc/nginx/conf.d/default.conf

# RUN rm /etc/nginx/sites-available/laradock.conf

COPY ./nginx/laradock.conf /etc/nginx/sites-available/

RUN ln -s /etc/nginx/sites-available/laradock.conf /etc/nginx/sites-enabled/


# Expose port 80
EXPOSE 80

# Start Nginx
CMD ["nginx", "-g", "daemon off;"]


